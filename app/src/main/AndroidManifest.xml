<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tsechacksapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.CALL_PHONE"/>



    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tsechacksapp">
        <activity android:name=".login.LoginActivity"
            android:exported="false"/>
        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <activity
            android:name=".game.SuccessActivity"
            android:exported="false" />
        <activity
            android:name=".doctor.AddMedicine"
            android:exported="false" />
        <activity
            android:name=".CollapsingtoolbarActivity"
            android:exported="false" />
        <activity
            android:name=".doctor.PatientInfo"
            android:exported="false" />
        <activity
            android:name=".doctor.MainActivityDoctor"
            android:exported="false" />
        <activity
            android:name=".Family.MainActivityFamily"
            android:exported="false" />
        <activity
            android:name=".doctor.MainMainDoctorActivity"
            android:exported="false" />
        <activity
            android:name=".game.GameMainActivity"
            android:exported="true" />
        <activity
            android:name=".splashscreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>